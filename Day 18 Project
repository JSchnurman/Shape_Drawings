from turtle import Turtle, Screen

tim = Turtle()
tim.shape("turtle")    # changed to a turtle shape
tim.color("blue")      # changed the color of the turtle to blue

# Draw a Square
for _ in range(4):
    tim.forward(100)
    tim.left(90)

# Draw a Dashed line
for _ in range(15):
    tim.forward(10)
    tim.penup()
    tim.forward(10)
    tim.penup()

screen = Screen()
screen.exitonclick()

